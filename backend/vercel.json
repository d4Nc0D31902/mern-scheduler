{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/me",
      "dest": "/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js",
      "headers": {
        "Access-Control-Allow-Origin": "https://mern-scheduler-frontend.vercel.app"
      }
    }
  ]
}
